{
  "name": "static-blogging-system",
  "version": "1.1.0",
  "description": "一个优雅的、支持 Markdown 的静态博客系统，支持 RSS 订阅和 SEO 优化",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "node src/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write \"src/**/*.{js,json,md}\"",
    "docs": "jsdoc -c jsdoc.json",
    "generate-sitemap": "node src/sitemap.js",
    "generate-rss": "node src/rss.js"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "chokidar": "^3.5.3",
    "compression": "^1.7.4",
    "ejs": "^3.1.9",
    "express": "^4.21.2",
    "front-matter": "^4.0.2",
    "fs-extra": "^11.2.0",
    "helmet": "^7.1.0",
    "highlight.js": "^11.11.1",
    "marked": "^12.0.0",
    "morgan": "^1.10.0",
    "rss": "^1.2.2",
    "yaml": "^2.3.4",
    "xml2js": "^0.6.2",
    "sitemap": "^7.1.1"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "jsdoc": "^4.0.2"
  },
  "keywords": [
    "blog",
    "static-site",
    "markdown",
    "node",
    "express",
    "ejs",
    "rss",
    "seo",
    "sitemap"
  ],
  "author": {
    "name": "Lcyys666",
    "url": "https://github.com/Bad-communication-group"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Bad-communication-group/Static-blogging-system.git"
  },
  "bugs": {
    "url": "https://github.com/Bad-communication-group/Static-blogging-system/issues"
  },
  "homepage": "https://github.com/Bad-communication-group/Static-blogging-system#readme"
}
